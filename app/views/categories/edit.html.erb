<h1>Edit Category</h1>
<%=form_for @category do |f|%>
  <div class="form-group">
    <%=f.label :title%>
    <%=f.text_field :title, class: "form-control"%>
  </div>

  <div class="form-group">
    <%=f.label :title%>
    <%=f,text_field :category_id, class: "form-control"%>
  </div>

  <div class="actions">
    <%=f.submit "Edit", class: "btn btn-primary"%>
  </div>
<%end%> 
<%=link_to "Back", categories_path%>